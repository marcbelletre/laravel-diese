{
    "name": "marcbelletre/laravel-diese",
    "description": "Laravel wrapper for Diese Software API",
    "keywords": [
        "diese",
        "laravel"
    ],
    "homepage": "https://github.com/marcbelletre/laravel-diese",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marc BellÃªtre",
            "email": "marc@pixelparfait.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "laravel/pint": "^1.7",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MarcBelletre\\LaravelDiese\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcBelletre\\LaravelDiese\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarcBelletre\\LaravelDiese\\LaravelDieseServiceProvider"
            ]
        }
    }
}
